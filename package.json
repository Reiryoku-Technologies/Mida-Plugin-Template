{
    "name": "@reiryoku/mida-plugin-template",
    "version": "1.0.0",
    "description": "A template for creating a Mida plugin.",
    "scripts": {
        "lint": "tslint --project .",
        "build": "tsc",
        "test": "npm run build && jest --verbose",
        "safe-build": "rm -rf ./build && npm run lint && npm run build && jest"
    },
    "imports": {
        "#*": "./build/src/*.js"
    },
    "exports": {
        ".": "./build/mida.plugin.js",
        "./*": "./build/src/*.js"
    },
    "main": "./build/mida.plugin.js",
    "devDependencies": {
        "@types/jest": "26.0.20",
        "@types/node": "12.7.2",
        "jest": "26.6.3",
        "tslint": "5.18.0",
        "typescript": "4.1.3"
    },
    "peerDependencies": {
        "@reiryoku/mida": "1.x"
    }
}
